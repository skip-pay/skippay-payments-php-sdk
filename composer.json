{
    "name": "skip-pay/skippay-payments-php-sdk",
    "description": "API documentation for Skip Pay partners.",
    "keywords": [
        "openapitools",
        "openapi-generator",
        "openapi",
        "php",
        "sdk",
        "rest",
        "api"
    ],
    "homepage": "https://dxheroes.io",
    "license": "unlicense",
    "authors": [
        {
            "name": "DX Heroes",
            "homepage": "https://dxheroes.io"
        }
    ],
    "require": {
        "php": ">=7.4",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "^6.2 || ^7.0"
    },
    "require-dev": {
        "fakerphp/faker": "^1.20",
        "illuminate/support": ">5.0.0",
        "php-parallel-lint/php-parallel-lint": "^1.2",
        "phpspec/prophecy-phpunit": "^2.0",
        "phpstan/phpstan": "^1.0.0",
        "phpunit/phpunit": "^8.0 | ^9.0",
        "squizlabs/php_codesniffer": "^3.7",
        "vimeo/psalm": "^4.5"
    },
    "autoload": {
        "psr-4": {
            "SkipPay\\" : "lib/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SkipPay\\Test\\" : "test/"
        }
    }
}
